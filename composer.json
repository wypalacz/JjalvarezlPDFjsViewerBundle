{
    "name": "jjalvarezl/pdfjs-viewer-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides an integration of pdf.js from mozilla with Symfony 2 framework and customizable parameters",
    "keywords": ["pdfjs", "Symfony", "Symfony2", "pdf viewer"],
    "license": "MIT",
    "authors": [
        {
            "name": "jjalvarezl",
            "email": "jhon.jairo.alvarez.londono@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3.2"
    },
    "autoload": {
        "psr-0": { "jjalvarezl\\PDFjsViewerBundle" : "" }
    },
    "target-dir" : "jjalvarezl/PDFjsViewerBundle",
    "minimum-stability": "dev",
    "extra" : {
        "branch-alias" : {
            "dev-master" : "@dev"
        }
    }
}
